{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "九章刷题小助手",
  "description": "帮你一键直达LeetCode / LintCode题解，从此找题解不愁，刷题成为享受。",
  "version": "1.3.1",
  "homepage_url":"http://www.jiuzhang.com/",

  "icons": {
    "128": "img/icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "page_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html",
    "default_title": "九章刷题小助手"
  },
  "permissions": [
    "declarativeContent",
    "*://*/jiuzhang.com/*",
    "*://*.leetcode.com/*",
    "*://*.leetcodechina.com/*",
    "*://*.leetcode-cn.com/*",
    "*://*.lintcode.com/*",
    "*://*.lintcode.cn/*"
  ]
}
